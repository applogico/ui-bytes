{
  "$schema": "https://unpkg.com/release-it/schema/release-it.json",
  "plugins": {
    "release-it-pnpm": {
      "disableRelease": true,
      "inFile": "CHANGELOG.md",
      "publishCommand": "vsce publish --no-dependencies"
    }
  },
  "git": {
    "commitMessage": "chore: release ${version}"
  }
}
